{
  "name": "sports-tickets",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --port 3000\" \"node dev-server.cjs\" \"stripe listen --forward-to localhost:3000/api/stripe-webhook\"",
    "dev:functions": "node dev-server.js",
    "dev:full": "vercel dev",
    "dev:ngrok": "ngrok http 3000",
    "build": "vite build",
    "preview": "vite preview",
    "vercel-build": "vite build",
    "test:fulfillment": "stripe trigger checkout.session.completed",
    "test:real": "stripe trigger checkout.session.completed --add checkout_session:metadata.admissionQuantity=2 --add checkout_session:metadata.parkingQuantity=1 --add checkout_session:metadata.buyerEmail=garetcrenshaw@gmail.com",
    "test:webhook": "node test-webhook.js",
    "deploy": "npm run build && vercel --prod"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^2.4.0",
    "@stripe/stripe-js": "^4.9.0",
    "@supabase/supabase-js": "^2.45.4",
    "concurrently": "^8.2.2",
    "dotenv": "^17.2.3",
    "http-proxy": "^1.18.1",
    "micro": "^10.0.1",
    "ngrok": "^5.0.0-beta.2",
    "qrcode": "^1.5.3",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "resend": "^4.0.0",
    "stripe": "^17.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.4.8"
  }
}
